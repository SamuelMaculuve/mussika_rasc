
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="zxx">
<head>
    <meta name="author" content="">
    <meta name="description" content="">
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8" />
    <meta http-equiv="Content-Security-Policy" content="upgrade-insecure-requests">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Mussika - Administração</title>


    <!-- Favicon -->
    <link rel="shortcut icon" href="~/Repository/images/logo/favicon_mussika 16x16.png">


    <link href="~/Content/library/css/jquery.dataTables.min.css" rel="stylesheet" />


    <!-- Style CSS -->
    <link rel="stylesheet" href="~/Content/library/css/stylesheet.css">
    <link rel="stylesheet" href="~/Content/library/css/style.css" id="colors">


    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Nunito:300,400,600,700,800&display=swap&subset=latin-ext,vietnamese" rel="stylesheet">

    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,500,600,700,800" rel="stylesheet" type="text/css">

    <link href="~/Content/library/css/jquery-confirm.min.css" rel="stylesheet" />

    <style>
        textarea {
            height: 500px;
        }
    </style>





</head>
<body>

    <!-- Wrapper -->
    <div id="main_wrapper">
        <header id="header_part" class="fixed fullwidth_block dashboard">
            <div id="header" class="not-sticky">
                <div class="container">
                    <div class="utf_left_side">
                        <div id="logo"> <a href="/Provedor/DashBoard"><img src="~/Repository/images/logo/logo 38 x 50.png" alt=""></a> <a href="/Provedor/DashBoard" class="dashboard-logo"><img src="~/Repository/images/logo/logo 38 x 50.png" alt=""></a> </div>
                        <div class="nav_responsive"> <i class="fa fa-reorder menu-trigger"></i> </div>
                        <div class="clearfix"></div>
                    </div>
                    <div class="utf_right_side">
                        <div comLogin class="header_widget">
                            <div class="dashboard_header_button_item has-noti js-item-menu">
                                <i class="sl sl-icon-bell"></i>
                                <div class="dashboard_notifi_dropdown js-dropdown">
                                    <div class="dashboard_notifi_title">
                                        <p>Você Tem 2 Notificações</p>
                                    </div>
                                    <div class="dashboard_notifi_item">
                                        <div class="bg-c1 red">
                                            <i class="fa fa-check"></i>
                                        </div>
                                        <div class="content">
                                            <p>Pedido de suporte</p>
                                            <span class="date">2 horas atrás </span>
                                        </div>
                                    </div>

                                    <div class="dashboard_notify_bottom text-center pad-tb-20">
                                        <a href="#">Ver todas as Notificações </a>
                                    </div>
                                </div>
                            </div>
                            <div class="utf_user_menu">
                                <div class="utf_user_name"><span><img src="~/Repository/images/perfil.png" height="40" width="40" alt=""></span>Olá,  @Session["nome"].ToString()!</div>
                                <ul>
                                    <li><a href="#"><i class="sl sl-icon-layers"></i> Mensagens</a></li>
                                    <li><a href="#"><i class="sl sl-icon-user"></i> Meu Perfil</a></li>
                                    <li><a href="/Login/RedifinirSenha?usuario=@Session["usuario"].ToString()"><i class="sl sl-icon-envelope-open"></i> Redifinir Senha</a></li>
                                    <li><a href="/Login/Sair"><i class="sl sl-icon-power"></i>  Sair</a></li>
                                </ul>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </header>
        <div class="clearfix"></div>
        <!-- Dashboard -->
        <div id="dashboard">
           
            <a href="#" class="utf_dashboard_nav_responsive"><i class="fa fa-reorder"></i> Admin Menu</a>
            <div class="utf_dashboard_navigation">
                <div class="utf_dashboard_navigation_inner_block">
                    <ul>

                        @if (bool.Parse(Session["musica"].ToString()) == true)
                        {

                            <li>
                                <a href="#"><i class="sl sl-icon-layers"></i>Musica</a>
                                <ul>

                                    <li><a href="/ProvedorMusica/Index?id=@Session["idusario"]">Cadastro/Lista</a></li>
                                    <li><a href="/Musica/gestao?layout=_LayoutProvedor">Artistas</a></li>

                                </ul>
                            </li>
                        }

                        @if (bool.Parse(Session["acomodacao"].ToString()) == true)
                        {

                            <li>
                                <a href="#"><i class="sl sl-icon-layers"></i>Acomodação</a>
                                <ul>

                                    <li><a href="/ProvedorAcomodacao/Index?id=@Session["idusario"]">Cadastro/Lista</a></li>
                                    <li><a href="/ProvedorAcomodacao/Reservas">Reservas</a></li>

                                </ul>
                            </li>

                        }
                        @if (bool.Parse(Session["bazar"].ToString()) == true)
                        {
                            <li class=""><a href="/ProvedorBazar/Index?id=@Session["idusario"]"><i class="sl sl-icon-layers"></i> Bazar</a></li>
                        }
                        @if (bool.Parse(Session["loja"].ToString()) == true)
                        {
                            <li class=""><a href="/ProvedorLoja/Index?id=@Session["idusario"]"><i class="sl sl-icon-layers"></i> Lojas</a></li>
                        }
                        @*@if (bool.Parse(Session["taxi"].ToString()) == true)
        {
            <li class=""><a href="#"><i class="sl sl-icon-layers"></i> Taxi</a></li>
        }*@
                        @if (bool.Parse(Session["restaurante"].ToString()) == true)
                        {
                            <li class=""><a href="/ProvedorRestaurante/Index?id=@Session["idusario"]"><i class="sl sl-icon-layers"></i> Restaurante</a></li>
                        }
                        @if (bool.Parse(Session["arte_cultura"].ToString()) == true)
                        {
                            <li class=""><a href="/ProvedorArteCultura/Index?id=@Session["idusario"]"><i class="sl sl-icon-layers"></i> Arte e Cultura</a></li>
                        }
                        @if (bool.Parse(Session["evento"].ToString()) == true)
                        {
                            <li>
                                <a href="#"><i class="sl sl-icon-ghost"></i>Eventos</a>
                                <ul>

                                    <li class=""><a href="/ProvedorEvento/Index"><i class="sl sl-icon-layers"></i> Evento</a></li>
                                    <li><a href="/ProvedorEvento/Bilhetes"><i class="sl sl-icon-ghost"></i>Bilhetes</a></li>
                                </ul>
                            </li>

                        }
                        @if (bool.Parse(Session["turismo"].ToString()) == true)
                        {
                            <li class=""><a href="/ProvedorTurismo/Index"><i class="sl sl-icon-layers"></i> Turismo</a></li>
                        }
                        @if (bool.Parse(Session["farmacia"].ToString()) == true)
                        {
                            <li class=""><a href="/Saude/gestao"><i class="sl sl-icon-layers"></i> Farmacia</a></li>
                        }
                        @if (bool.Parse(Session["clinica"].ToString()) == true)
                        {
                            <li>
                                <a href="#"><i class="sl sl-icon-ghost"></i>Clinica</a>
                                <ul>

                                    <li class=""><a href="/Saude/gestao"><i class="sl sl-icon-layers"></i> Clinica</a></li>
                                    <li><a href="/PubSaude/Consultas"><i class="sl sl-icon-ghost"></i>Consultas</a></li>
                                </ul>
                            </li>

                        }
                        @if (bool.Parse(Session["transportadora"].ToString()) == true)
                        {
                            <li>
                                <a href="#"><i class="sl sl-icon-ghost"></i>Transportadora</a>
                                <ul>

                                    <li class=""><a href="/Transporte/Index"><i class="sl sl-icon-layers"></i> Cadastro/Lista</a></li>
                                   
                                </ul>
                            </li>

                        }
                        @if (bool.Parse(Session["pagamento_servico"].ToString()) == true)
                        {
                            <li class=""><a href="#"><i class="sl sl-icon-layers"></i> Pagamento de Serviço</a></li>
                        }

                        <li>
                            <a href="#"><i class="sl sl-icon-ghost"></i>Vendas</a>
                            <ul>

                                <li><a href="/Provedor/vendas"><i class="sl sl-icon-ghost"></i> Minhas Vendas</a></li>
                                @*<li><a href="/Provedor/vendas"><i class="sl sl-icon-ghost"></i> Pedidos em aberto</a></li>*@
                            </ul>
                        </li>

                        <li><a href="/inicio/principal"><i class="sl sl-icon-power"></i> Sair</a></li>
                    </ul>
                </div>
            </div>

          
            <script type="text/javascript" src="~/Content/library/scripts/jquery-2.2.0.min.js"></script>
            <script src="~/Content/tinymce.min.js"></script>
            @*<script src="https://cdn.tiny.cloud/1/mq0mspo0y3nqips9n8tsf7emvyafkozh6ok987kiwed1dejw/tinymce/5/tinymce.min.js" referrerpolicy="origin"></script>*@
            <script type="text/javascript" src="~/Content/library/scripts/jpanelmenu.min.js"></script>
            <script type="text/javascript" src="~/Content/library/scripts/chosen.min.js"></script>
            <script type="text/javascript" src="~/Content/library/scripts/slick.min.js"></script>
            <script type="text/javascript" src="~/Content/library/scripts/rangeslider.min.js"></script>
            <script type="text/javascript" src="~/Content/library/scripts/magnific-popup.min.js"></script>
            <script type="text/javascript" src="~/Content/library/scripts/waypoints.min.js"></script>
            <script type="text/javascript" src="~/Content/library/scripts/counterup.min.js"></script>
            <script type="text/javascript" src="~/Content/library/scripts/jquery-ui.min.js"></script>
            <script type="text/javascript" src="~/Content/library/scripts/tooltips.min.js"></script>
            <script type="text/javascript" src="~/Content/library/scripts/color_switcher.js"></script>
            <script type="text/javascript" src="~/Content/library/scripts/jquery_custom.js"></script>

            <script type="text/javascript" src="~/Content/library/scripts/jquery.dataTables.min.js"></script>


            <script type="text/javascript" src="~/Content/library/scripts/loadingoverlay.min.js"></script>
            <script type="text/javascript" src="~/Content/library/scripts/dropzone.js"></script>

            <script src="http://maps.google.com/maps/api/js?sensor=false&amp;language=en"></script>
            <script src="~/Content/library/scripts/infobox.min.js"></script>

            <script src="~/Content/library/scripts/markerclusterer.js"></script>
            <script src="~/Content/library/scripts/maps.js"></script>
            @*<script src="~/Content/library/scripts/quantityButtons.js"></script>*@
            <script src="~/Content/library/scripts/moment.min.js"></script>
            <script src="~/Content/library/scripts/daterangepicker.js"></script>

            
            <div class="utf_dashboard_content">

                @RenderBody()


            </div>
            
        </div>
    </div>

    <!-- Scripts -->

    <script src="~/Content/library/scripts/jquery-confirm.min.js"></script>

    <script src="~/Content/library/Dialogos.js"></script>






    <script type="text/javascript">
        tinymce.init({
            selector: 'textarea',
            content_style: 'body { font-size:14px; }'
        });

        var refreshSn = function () {
            var time = 240000; // 10 mins
            setTimeout(
                function () {
                    $.ajax({
                        url: '/inicio/actualizarSession',
                        cache: false,
                        complete: function () { refreshSn(); }
                    });
                },
                time
            );
        };

        // Call in page
        refreshSn()
    </script>

</body>
</html>
