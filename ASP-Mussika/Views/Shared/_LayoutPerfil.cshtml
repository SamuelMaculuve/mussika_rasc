        tinymce.init({
            selector: 'textarea',
            content_style: 'body { font-size:14px; }'
        });

        var refreshSn = function () {
            var time = 240000; // 10 mins
            setTimeout(
                function () {
                    $.ajax({
                        url: '/inicio/actualizarSession',
                        cache: false,
                        complete: function () { refreshSn(); }
