                                    <li><a href="@Url.Action("Index","AdminEvento")">Eventos</a></li>
                                    <li><a href="~/AdminTurismo/Index">Turismo</a></li>
                                    <li><a href="@Url.Action("gestao","Saude")">Saúde</a></li>
                                    <li><a href="@Url.Action("Index","Usuario")">Serviços Mussika</a></li>



                                </ul>
                            </li>
                            <li>
                                <a href="#"><i class="sl sl-icon-layers"></i>Cadastros 3</a>
