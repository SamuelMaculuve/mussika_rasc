                                    <li><a href="~/empresa/comopagar">Como Pagar</a></li>
                                    <li><a href="@Url.Action("comopagar","comopagar")">Ajuda</a></li>

                                </ul>
                            </li>


                            <li><a href="#"><i class="sl sl-icon-docs"></i> Reservas</a></li>
                        }



                        <li><a href="~/administracao/Compras"><i class="sl sl-icon-heart"></i> Vendas</a></li>
                        
                        <li><a href="~/Usuario/Index"><i class="sl sl-icon-user"></i> Utilizadores</a></li>
                        <li><a href="#"><i class="sl sl-icon-user"></i> Meu Perfil</a></li>
                        <li><a href="#"><i class="sl sl-icon-key"></i> Trocar Senha</a></li>
                        <li><a href="/inicio/principal"><i class="sl sl-icon-power"></i> Sair</a></li>
                    </ul>
                </div>
            </div>

            <!-- Scripts -->
            @*<script type="text/javascript" src="~/Content/library/scripts/jquery-3.4.1.min.js"></script>*@
            <script type="text/javascript" src="~/Content/library/scripts/jquery-2.2.0.min.js"></script>
            <script src="~/Content/tinymce.min.js"></script>
            @*<script src="https://cdn.tiny.cloud/1/mq0mspo0y3nqips9n8tsf7emvyafkozh6ok987kiwed1dejw/tinymce/5/tinymce.min.js" referrerpolicy="origin"></script>*@
