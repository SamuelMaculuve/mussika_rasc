                                </ul>
                            </li>
                            <li>
                                <a href="#"><i class="sl sl-icon-layers"></i>Politicas</a>
                                <ul>

                                    <li><a href="~/empresa/Index">Empresa</a></li>
                                    <li><a href="~/Publicidade/Index">Publicidade</a></li>
                                    @*<li><a href="@Url.Action("Criar","acomodacao")">Categorias</a></li>*@
