                        if (item.caminho.Trim() == "noimage.png") { continue; }
                        <a href="@Url.Content("~/Repository/images/Acomodacoes/"+@item.caminho)" data-background-image="@Url.Content("~/Repository/images/Acomodacoes/"+@item.caminho)" class="item utf_gallery"></a>
                    }
                }
                else
                {
                    <a href="@Url.Content("~/Repository/images/bg/background.jpg")" data-background-image="@Url.Content("~/Repository/images/bg/background.jpg")" class="item utf_gallery"></a>
